        "@teambit/base-ui.routing.routing-provider": "1.0.0",
        "@teambit/base-ui.surfaces.card": "1.0.1",
        "@teambit/base-ui.text.paragraph": "1.0.3",
        "@teambit/component-id": "0.0.369",
