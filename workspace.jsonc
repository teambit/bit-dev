        "@teambit/design.ui.heading": "1.0.7",
        "@teambit/design.ui.icon-button": "1.0.10",
        "@teambit/design.ui.input.icon-text": "0.0.7",
        "@teambit/design.ui.input.toggle": "1.0.7",
